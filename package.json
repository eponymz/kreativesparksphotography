    "express-messages": "^1.0.1",
    "express-session": "^1.15.6",
    "mongodb": "^3.0.6",
    "mongoose": "^5.0.14",
    "nodemailer": "^4.6.6",
    "nodemon": "^1.17.3",
    "npm": "^6.1.0",
    "passport": "^0.4.0",
    "passport-google-oauth20": "^1.0.0",
    "req-flash": "0.0.3",
    "standard-version": "^4.4.0"
  }
